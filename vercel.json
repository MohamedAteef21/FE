{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/restaurant-website/browser",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://bashawat.runasp.net/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

